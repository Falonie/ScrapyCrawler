mongodump --archive=newseed_种子_crawled_result.gz --gzip --db Falonie --collection newseed_种子_crawled_result

mongorestore --gzip --archive=newseed_种子_crawled_result.gz --db Falonie

mongoexport --db Falonie --collection newseed_种子_urls --type=csv --fields url --out /media/salesmind/Other/PycharmProjects/work/NewSeed/newseed_种子_urls.csv

mongoexport --db Falonie --collection newseed_天使_urls --type=csv --fields url --out E:\PycharmProjects\mind-ai-data\python\trunk\FalonieRepository\NewSeed\newseed_天使_urls.csv

